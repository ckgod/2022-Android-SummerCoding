<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ckgod.MainActivity"
    android:padding="20dp"
    android:background="@color/white">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnD11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/d11210920"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/btnGok"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnGok"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/gok5959"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/btnD11"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnLee"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/leesc0839"
        app:layout_constraintStart_toStartOf="@id/btnD11"
        app:layout_constraintTop_toBottomOf="@id/btnD11"
        app:layout_constraintEnd_toStartOf="@id/btnYes"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnYes"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/yesxon"
        app:layout_constraintStart_toEndOf="@id/btnLee"
        app:layout_constraintTop_toTopOf="@id/btnLee"
        app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCkg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/ckgod"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>